{
  "name": "authy-desktop-export",
  "version": "0.1.0",
  "description": "utility to export TOTP secrets from a running authy desktop",
  "author": "Authier @authierpm",
  "bin": {
    "authy-desktop-export": "./bin/run"
  },
  "homepage": "https://github.com/authier-pm/authy-desktop-export",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "authier-pm/authy-desktop-export",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "chrome-remote-interface": "^0.31.2"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/chrome-remote-interface": "^0.31.4",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.16",
    "chai": "^4",
    "eslint": "^8.8.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "husky": "^7.0.4",
    "mocha": "^9",
    "oclif": "^2",
    "prettier": "^2.6.1",
    "pretty-quick": "^3.1.3",
    "shx": "^0.3.4",
    "ts-node": "^10.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "oclif": {
    "bin": "authy-desktop-export",
    "dirname": "authy-desktop-export",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/authier-pm/authy-desktop-export/issues",
  "keywords": [
    "authy"
  ],
  "types": "dist/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
