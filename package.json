{
  "name": "authy-desktop-export",
  "version": "1.0.1",
  "description": "utility to export TOTP secrets from a running authy desktop",
  "author": "Authier @authierpm",
  "bin": {
    "authy-desktop-export": "./bin/run"
  },
  "homepage": "https://github.com/authier-pm/authy-desktop-export",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "authier-pm/authy-desktop-export",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.16.0",
    "@oclif/plugin-help": "^5.1.12",
    "chrome-remote-interface": "^0.31.3"
  },
  "devDependencies": {
    "@oclif/test": "^2.1.1",
    "@types/chai": "^4.3.3",
    "@types/chrome-remote-interface": "^0.31.4",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.14",
    "chai": "^4.3.6",
    "eslint": "^8.24.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "oclif": "^3.1.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  },
  "oclif": {
    "bin": "authy-desktop-export",
    "dirname": "authy-desktop-export",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "watch": "tsc -b -w",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/authier-pm/authy-desktop-export/issues",
  "keywords": [
    "authy"
  ],
  "types": "dist/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
